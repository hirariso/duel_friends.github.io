<div class="home_page">
    <div class="home_container">
        <h1>ユーザー登録</h1>
        <div class="error_message">
        <% if @error_message%>
        <%= @error_message%>
        <%end%>
        </div>
        <%=form_tag("/users/create")do %>
        <input name="name" value="<%=params[:name]%>" placeholder="ハンドルネーム">
        <p><input name="password" type="password" placeholder="パスワード"></p>
        <input type="submit" value="新規登録" class="btn">
        <%end%>
    </div>
</div>